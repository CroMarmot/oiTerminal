            <ul class="second-level-menu-list">
              <li class="selectedLava source-button"><a href="#"
                  onclick='$("#facebox .source-and-history-div .source").show(); $("#facebox .source-and-history-div .history").hide(); return false;'>source</a>
              </li>
              <li class="history-button"><a href="#"
                  onclick='$("#facebox .source-and-history-div .source").hide(); $("#facebox  .source-and-history-div .history").show(); return false;'>history</a>
              </li>
            </ul>
            <hr style="margin-top: -3px;" />
          </div>

          <div class="source" style="display: none;">
            <div class="holder">
              &nbsp;
            </div>
            <div style="text-align: center;">
              <button class="challenge-button" style="padding: 0.25em 0.5em; margin-top: 1em;">Hack!</button>
            </div>
          </div>
          <div class="history" style="display: none;"></div>
        </div>

        <div class="test-for-popup-proto" style="display:none;">
          <div class="test-header">
            Test: #<span class="test">&nbsp;</span>,
            time: <span class="time">&nbsp;</span> ms.,
            memory: <span class="memory">&nbsp;</span> KB,
            exit code: <span class="exitCode">&nbsp;</span>,
            checker exit code: <span class="checkerExitCode">&nbsp;</span>,
            verdict: <span class="verdict">&nbsp;</span>
          </div>
          <div style="font-size:1.1rem;">
            <div class="popup-input-div">
              <div>Input</div>
              <pre class="input">&nbsp;</pre>
            </div>
