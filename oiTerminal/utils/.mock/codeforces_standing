    <script type="text/javascript">
      $(function () {
        window._ftaa = "57an8wl7xgv2vmeal2";
      })
    </script>

    <script type="text/javascript">
      $(function () {
        window._bfaa = "6557b941d482c3997936e1d25f163c23";
      });
    </script>
    <script type="application/javascript">
      window.setTimeout(function () {
        var url = location.href;
        var hashCode32 = Math.abs(url.hashCode()).toString();
        var hashCode = "p" + hashCode32 + Math.abs((hashCode32 + url).hashCode());
        var pvs = Codeforces.getFromStorage("pvs" + hashCode);
        if (!pvs) {
          $.post("/data/apv", {
            ftaa: window._ftaa,
            bfaa: window._bfaa,
            url: url
          }, function (json) {
            if (json["success"] === "true") {
              Codeforces.putToStorage("pvs" + hashCode, true);
            }
          }, "json");
        }
      }, 5000);
    </script>


