        <script type="text/javascript">
          $(document).ready(function () {
            var participantId = window.location.hash.replace("#", "") || "";
            if (participantId != "" && participantId[0] == 'p') {
              var row = $("tr[participantId='" + participantId.substring(1) + "']");
              $('html, body').animate({
                'scrollTop': Math.max(0, row.offset().top - ($(window).height() - row.height()) / 2)
              });
            }
          });
        </script>

