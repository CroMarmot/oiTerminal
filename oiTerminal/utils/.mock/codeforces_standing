        <style>
          .source-popup-diagnostics {
            font-weight: bold;
            margin-bottom: 1em;
          }

          .source-popup-diagnostics .diagnosticsMessageType {
            color: #E65100;
          }

          #program-source-text-copy {
            cursor: pointer;
            color: blue;
            text-decoration: underline;
          }
        </style>
        <div class="source-popup"
          style="display:none;width:950px;height:600px;overflow:auto;padding:1em;margin:1em;font-size:1.2rem;">
          <span class="source-popup-header" style="">&nbsp;</span>
          <pre
            style="background-color: #eff0f1;"><code class="source-popup-source prettyprint linenums"><img src="//codeforces.org/s/28957/images/indicator.gif"/></code></pre>
        </div>

        <script type="text/javascript">
          var source = "";
          $(document).ready(function () {
            showSubmissionSourceCode = function (submissionId) {
              $.post("/data/submitSource", { submissionId: submissionId }, function (data) {
                var nop = $(".next-or-prev-prototype").clone().removeClass("next-or-prev-prototype").addClass("next-or-prev").css("display", "block");
                Codeforces.facebox(".source-popup:first", "//codeforces.org/s/28957");
                var sourcePre = $("#facebox").find(".source-popup-source:first");
                if (data["prettifyClass"]) {
                  sourcePre.addClass(data["prettifyClass"]);
                }
                // sourcePre.empty();
                // sourcePre.append($("<code></code>").text(data["source"]));
                if (data.diagnosticsMessage) {
                  var icon = "<i title=\"Diagnostics Hint\" class=\"icon-warning-sign diagnostics-icon\"></i> ";
                  $("#facebox .source-popup").prepend($("<div class=\"source-popup-diagnostics\"></div>").html(icon + data.diagnosticsMessage));
                }
                if (data["source"]) {
                  sourcePre.text(data["source"]);
                } else if (data["sourceHtml"]) {
                  sourcePre.html(data["sourceHtml"]);
                }
                sourcePre.removeClass("prettyprinted");
                var header = "By " + data["partyName"]
                  + ", contest: " + data["contestName"]
                  + ", problem: " + data["problemName"]
                  + ", " + data["verdict"] + ", " + "<a title='Direct Link' href=\"" + data["href"] + "\">#</a>"
                  + ", " + "<span id=\"program-source-text-copy\" title='Copy' href='#'>Copy</span>";
                if (data["offerChallenge"] === "true") {
                  header += ", <a href=\"" + data["challengeLink"] + "\">hack it!</a>";
                }
                header += "<hr/>";

                $("#facebox").find(".source-popup-header").html(header);
                source = data["source"];

                if ($("#facebox").find(".footer .pagination").size() === 0) {
                  $("#facebox").find(".footer").append(nop);
                }
                var p = $("#facebox").find(".footer .pagination");

                if (data["prevId"]) {
                  p.find(".prev").attr("href", "#").click(function () {
                    showSubmissionSourceCode(data["prevId"]);
                    return false;
                  });
                }
                if (data["nextId"]) {
                  p.find(".next").attr("href", "#").click(function () {
                    showSubmissionSourceCode(data["nextId"]);
                    return false;
                  });
                }

                prettyPrint();
                if (data.diagnosticsLine) {
                  $("#facebox .source-popup code li:nth-child(" + data.diagnosticsLine + ")").css("background", "#E57373");
                }

                var testCount = data["testCount"] ? parseInt(data["testCount"]) : 0;
                if (testCount > 0) {
                  $("#facebox").find(".source-popup").append("<hr/><h4>&rarr;Judgement Protocol</h4>");
                }
                for (var i = 1; i <= testCount; i++) {
                  if (data["verdict#" + i]) {
                    var e = $("div.test-for-popup-proto").clone()
                      .removeClass("test-for-popup-proto").addClass("test-for-popup")
                      .css("display", "block");
                    e.find(".test").text(i);
                    e.find(".time").text(data["timeConsumed#" + i]);
                    e.find(".memory").text(data["memoryConsumed#" + i] ? parseInt(data["memoryConsumed#" + i]) / 1024 : "");
                    e.find(".exitCode").text(data["exitCode#" + i]);
                    e.find(".checkerExitCode").text(data["checkerExitCode#" + i]);
                    e.find(".verdict").text(data["verdict#" + i]);

                    if (data["input#" + i]) {
                      e.find(".input").text(data["input#" + i]);
                    } else {
                      e.find(".popup-input-div").hide();
                    }

                    if (data["output#" + i]) {
                      e.find(".output").text(data["output#" + i]);
                    } else {
                      e.find(".popup-output-div").hide();
                    }

                    if (data["answer#" + i]) {
                      e.find(".answer").text(data["answer#" + i]);
                    } else {
                      e.find(".popup-answer-div").hide();
                    }

                    if (data["checkerStdoutAndStderr#" + i]) {
                      e.find(".checker").text(data["checkerStdoutAndStderr#" + i] ? data["checkerStdoutAndStderr#" + i] : " ");
                    } else {
                      e.find(".popup-checker-div").hide();
                    }

                    if (data["diagnostics#" + i]) {
                      e.find(".diagnostics").text(data["diagnostics#" + i]);
                    } else {
                      e.find(".popup-diagnostics-div").hide();
                    }
                    $("#facebox").find(".source-popup").append(e);
                  }
                }
              }, "json");
            };

            $(".prettyprint.source-popup-source").each(function () {
              var clipboard = new Clipboard('#program-source-text-copy', {
                text: function (trigger) {
                  return source;
                }
              });

              clipboard.on('success', function (e) {
                Codeforces.showMessage("The source code has been copied into the clipboard");
                e.clearSelection();
              });
            });

            $(".change-participant-type-link").click(function () {
              var contestId = $(this).attr("data-contestId");
              var targetParticipantId = $(this).attr("data-participantId");

              var changeParticipantType = function () {
                $.post("/data/changeParticipantType",
                  { contestId: contestId, targetParticipantId: targetParticipantId },
                  function (response) {
                    if (response.success == "true") {
                      Codeforces.reload();
                    } else {
                      Codeforces.showMessage("Changing participant type is failed");
                    }
                  },
                  "json"
                );
              };

              Codeforces.confirm(
                "Are you sure you want to change participant type from virtual to practice (this operation is not reversible and may take a few seconds)?", changeParticipantType, function () { }, "OK", "No"
              );

              return false;
            });

            $(".participant-devirtualize-link").click(function () {
              var contestId = $(this).attr("data-contestId");
              var targetParticipantId = $(this).attr("data-participantId");

              var changeParticipantType = function () {
                $.post("/data/changeParticipantType",
                  { action: "devirtualize", contestId: contestId, targetParticipantId: targetParticipantId },
                  function (response) {
                    if (response.success == "true") {
                      Codeforces.reload();
                    } else {
                      Codeforces.showMessage("Changing participant type is failed");
                    }
                  },
                  "json"
                );
              };

              Codeforces.confirm(
                "Are you sure you want to devirtualize this participant (this operation is not reversible)?", changeParticipantType, function () { }, "OK", "No"
              );

              return false;
            });
          });
        </script>

        <script type="text/javascript">
          $(document).ready(function () {
            var challengeLink = "/contest/1621/challenge/123456789";
            function handleContestantProblemHistory(participantId, problemId, historyDiv) {
              $.post("/data/standings",
                { participantId: participantId, problemId: problemId },
                function (response) {
                  $(".source-and-history-div .indicator").hide();

                  $.each(response, function (i, event) {
                    if (event.type !== "ROOM") {
                      historyDiv.append(event.contestTime).append("&nbsp;&nbsp;");
                      if (event.type === "PROBLEM_LOCK") {
                        historyDiv.append("<span style=\"color: darkorange; font-weight: bold;\">Problem locked</span>");
                      } else if (event.type === "SUBMIT") {
                        historyDiv.append(event.verdict);
                      } else if (event.type === "CHALLENGE_BY_PARTY") {
                        historyDiv.append(event.verdict).append(" of ").append(event.otherParty).append("'s solution");
                      } else if (event.type === "CHALLENGE_FOR_PARTY") {
                        historyDiv.append(event.verdict).append(" by ").append(event.otherParty);
                      }
                      if (event.type !== "PROBLEM_LOCK" && event.submissionId > 0) {
                        historyDiv.append(" &rarr; <a submissionId =\"" + event.submissionId + "\" class=\"showSource\" href=\"#\">" + event.submissionId + "</a>");
                        if (event.offerChallenge === "true") {
                          var link = challengeLink.replace("123456789", event.submissionId);
                          historyDiv.append(" &rarr; <a href=\"" + link + "\">hack it!</a>");
                        }
                      }
                      historyDiv.append("<br/>");
                    }
                  });

                  historyDiv.find(".showSource").click(function () {
                    showSubmissionSourceCode($(this).attr("submissionId"));
                    return false;
                  });
                },
                "json"
              );
            }

            var loop = false;
            var intervalId = false;

            function unsetInterval() {
              if (intervalId != false) {
                window.clearInterval(intervalId);
                loop = false;
                intervalId = false;
              }
            }

            function showSource(participantId, problemId, container, submissionId) {
              function show(submissionId) {
                $(".source", container).show();
                $(".menu", container).show();
                $(".history", container.parent()).hide();
                $(".second-level-menu-list", container.parent()).lavaLamp({
                  fx: "backout",
                  speed: 1000
                });

                updateLastViewedSubmission(participantId, problemId, submissionId);

                $(".challenge-button", container).click(function () {
                  var challengeLink = "/contest/1621/challenge/0";
                  challengeLink = challengeLink.substring(0, challengeLink.lastIndexOf('/') + 1) + submissionId;
                  Codeforces.redirect(challengeLink);
                });

                var loopCount = 0;
                loop = true;

                $.ajaxSetup({
                  error: function (request, status, thrown) {
                    // No operations.
                  }
                });

                intervalId = window.setInterval(function () {
                  if (loop) {
                    if (loopCount > 30) {
                      unsetInterval();
                      $(".source", container).html("<h2>Timeout: please, reopen the submission</h2>");
                    } else {
                      $.post("/data/challenge", { submissionId: submissionId, action: "checkSubmission" }, function (json) {
                        // No operations.
                      }, "json").fail(function (xhr) {
                        var status = xhr.status;
                        if (status == 403) {
                          unsetInterval();
                          alert("The solution has been hacked or resubmitted");
                        }
                      });
                      loopCount++;
                    }
                  }
                }, 15000);
              }
              $.post("/data/challenge",
                { participantId: participantId, problemId: problemId, action: "getChallengeTryIframe" },
                function (response) {
                  $(".source .holder", container).html($("<iframe width='720' height='510'></iframe>").attr("src", response["url"]));
                  var submissionId = response["submissionId"];
                  show(submissionId);
                },
                "json"
              );
            }

            var lightRed = "#FFE3E3";
            var lightGreen = "#D4EDC9";
            var darkBlue = "#E3E3FF";

            var lastViewedSubmission = Codeforces.getFromStorage("lastViewedSubmission", "");
            if (lastViewedSubmission != "") {
              var lastViewedParticipantId = parseInt(lastViewedSubmission.split(' ')[0]);
              var lastViewedProblemId = parseInt(lastViewedSubmission.split(' ')[1]);

              $("tr").filter(function () { return $(this).attr("participantId") != undefined; }).each(function () {
                var participantId = $(this).attr("participantId");
                $(this).find("td").filter(function () { return $(this).attr("problemId") != undefined; }).each(function () {
                  var problemId = $(this).attr("problemId");
                  var submissionId = $(this).attr("acceptedSubmissionId");
                  var viewedSubmissionId = Codeforces.getFromStorage(getStorageKey(participantId, problemId), -1);
                  if (viewedSubmissionId == submissionId) {
                    if (participantId == lastViewedParticipantId && problemId == lastViewedProblemId) {
                      $(this).css("background-color", lightRed);
                    } else {
                      $(this).css("background-color", lightGreen);
                    }
                  } else if (viewedSubmissionId != -1) {
                    $(this).css("background-color", darkBlue);
                  }
                });
              });
            }

            function getStorageKey(participantId, problemId) {
              return "viewedSubmission[" +
                "participantId=" + participantId + ", " +
                "problemId=" + problemId + "]";
            }

            function updateLastViewedSubmission(participantId, problemId, submissionId) {
              var lastViewedSubmission = Codeforces.getFromStorage("lastViewedSubmission", "");
              if (lastViewedSubmission.length != "") {
                var lastViewedParticipantId = parseInt(lastViewedSubmission.split(' ')[0]);
                var lastViewedProblemId = parseInt(lastViewedSubmission.split(' ')[1]);
                $("tr[participantId=" + lastViewedParticipantId + "]").
                  find("td[problemId=" + lastViewedProblemId + "]").each(function () {
                    if ($(this).attr("acceptedSubmissionId") ==
                      Codeforces.getFromStorage(getStorageKey(lastViewedParticipantId, lastViewedProblemId), -1)) {
                      $(this).css("background-color", lightGreen);
                    }
                  });
              }
              Codeforces.putToStorage("lastViewedSubmission", participantId + " " + problemId);
              Codeforces.putToStorage(getStorageKey(participantId, problemId), submissionId);
              $("tr[participantId=" + participantId + "]").
                find("td[problemId=" + problemId + "]").css("background-color", lightRed);
            }

            function handleContestantProblem(td) {
              Codeforces.facebox(".source-and-history-div", "//codeforces.org/s/28957");
              $(".source-and-history-div .indicator").show();

              var container = $("#facebox .source-and-history-div");

              $(".menu", container).hide();
              $(".source", container).hide();
              $(".history", container).hide();
              $(".source pre", container).text("");
              $(".history", container).text("");

              var participantId = td.parent().attr("participantId");
              var problemId = td.attr("problemId");

              handleContestantProblemHistory(participantId, problemId, $(".history"));

              showSource(participantId, problemId, container, td.attr("acceptedsubmissionid"));
              $(".history", container).show();
            }

            function handleContestantHistory(td) {
              Codeforces.facebox(".history-div", "//codeforces.org/s/28957");
              $(".history-div .indicator").show();

              var participantId = td.parent().attr("participantId");

              $.post("/data/standings",
                { participantId: participantId },
                function (response) {
                  $(".history-div .indicator").css("display", "none");
                  let $historyDiv = $("#facebox .history-div");
                  $historyDiv.text("");

                  $.each(response, function (i, event) {
                    $historyDiv.append(event.contestTime).append("&nbsp;&nbsp;");

                    if (event.type !== "ROOM") {
                      $historyDiv.append(event.problem).append("&nbsp;&nbsp;");
                    }

                    if (event.type === "PROBLEM_LOCK") {
                      $historyDiv.append("<span style=\"color: darkorange; font-weight: bold;\">has been locked</span>");
                    } else if (event.type === "ROOM") {
                      $historyDiv.append("Participant has been assigned to the ").append(event.room);
                    } else if (event.type === "SUBMIT") {
                      $historyDiv.append(event.verdict);
                    } else if (event.type === "CHALLENGE_BY_PARTY") {
                      $historyDiv.append(event.verdict).append(" of ").append(event.otherParty);
                    } else if (event.type === "CHALLENGE_FOR_PARTY") {
                      $historyDiv.append(event.verdict).append(" by ").append(event.otherParty);
                    }

                    if (event.type !== "PROBLEM_LOCK" && event.submissionId > 0) {
                      $historyDiv.append(" &rarr; <a submissionId =\"" + event.submissionId + "\" class=\"showSource\" href=\"#\">" + event.submissionId + "</a>");
                      if (event.offerChallenge === "true") {
                        let link = challengeLink.replace("123456789", event.submissionId);
                        $historyDiv.append(" &rarr; <a href=\"" + link + "\">hack it!</a>");
                      }
                    }
                    $historyDiv.append("<br/>");
                  });

                  $historyDiv.find(".showSource").click(function () {
                    showSubmissionSourceCode($(this).attr("submissionId"));
                    return false;
                  });
                },
                "json"
              );
            }

            $(".standings td[problemId]").bind("dblclick", function () {
              handleContestantProblem($(this));
              return false;
            }).swipe({
              touch: function (that, target) {
                if (target) {
                  var jqueryTarget = $(target);

                  if (jqueryTarget.is("a") || jqueryTarget.parent() && jqueryTarget.parent().is("a")) {
                    return true;
                  }
                }

                handleContestantProblem($(that));
                return false;
              }
            });

            $(".standings td[problemId]").click(function (e) {
              if (e.ctrlKey) {
                handleContestantProblem($(this));
              }
            });

            $(".standings .contestant-cell").bind("dblclick", function () {
              handleContestantHistory($(this));
              return false;
            }).swipe({
              touch: function (that, target) {
                if (target) {
                  var jqueryTarget = $(target);

                  if (jqueryTarget.is("a") || jqueryTarget.parent() && jqueryTarget.parent().is("a")) {
                    return true;
                  }
                }

                handleContestantHistory($(that));
                return false;
              }
            });

            /*$(".standings .contestant-cell").click(function (e) {
                if (e.ctrlKey) {
                    handleContestantHistory($(this));
                }
            });*/

            $(document).bind("close.facebox", function () {
              unsetInterval();
            });
          });
        </script>


        <script type="text/javascript">
          $(document).ready(function () {
            var insertFirst = $("table.participantRowTable").attr("insertFirst");
            if (insertFirst) {
              var tr = $("table.participantRowTable tr:first").clone();
              if (insertFirst == "true") {
                var insertAfterTr = $(".standings tr:first");
                insertAfterTr.after(tr);
              } else {
                var statisticsRow = $(".standings tr.standingsStatisticsRow");
                if (statisticsRow.length) {
                  statisticsRow.before(tr);
                } else {
                  var insertAfterTr = $(".standings tr:last");
                  insertAfterTr.after(tr);
                }
              }
            }

            $(".standings").each(function () {
              $(this).find("tr:first th").addClass("top");
              $(this).find("tr:last td").addClass("bottom");
              $(this).find("tr:even td").removeClass("dark");
              $(this).find("tr:odd td").addClass("dark");
              $(this).find("tr td:first-child, tr th:first-child").addClass("left");
              $(this).find("tr td:last-child, tr th:last-child").addClass("right");
            });

            $(".standings tr[participantId=126063551]").addClass("highlighted-row");
          });
        </script>

